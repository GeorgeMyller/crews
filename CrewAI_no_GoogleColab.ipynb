      "source": [
        "Agente 2"
      ],
      "metadata": {
        "id": "7Sqqq31k8Z1N"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "influencer = Agent(\n",
        "    role='Influencer Writer',\n",
        "    goal=\"Write catchy, emoji-filled posts within 1000 words\",\n",
        "    backstory=\"\"\"You are a influencer to like conucate science with the society, you carrer starts at 3 years ago.\n",
        "\"\"\",\n",
        "    verbose=True,\n",
        "    allow_delegation=True,\n",
        "    llm=llm\n",
        ")"
      ],
      "metadata": {
        "id": "ViA7Fb4Z8b6S"
      },
      "execution_count": 75,
      "outputs": []
